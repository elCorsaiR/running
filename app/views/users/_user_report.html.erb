  <div class="container">
    <a class="logo logo--footer orange" href="<%= root_path %>">
      <%= image_tag 'logo.png', alt: 'running' %> Estudio biomecánico en 3D de tu técnica de carrera</a>

    <div class="social">
      <%= link_to 'SOBRE NOSOTROS', 'http://imasdrunning.com', class: 'footer-link white' %>
      <%= link_to 'FAQ', 'http://imasdrunning.com/estudio-biomecanico-3d/preguntas-frecuentes/', class: 'footer-link white' %>
      <%= link_to 'CONTACTO', 'http://imasdrunning.com/#contacto', class: 'footer-link white' %>
      <div class="buttons_wrapper">
      <%= render 'shared/social_buttons' %>
      </div>
    </div>
  </div>
</footer>
</div>